<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="BaseSearchFolder" value="..\..\test"/> 
    <add key="SearchPaths" value="sample_source_files"/>
    <!-- Just directory name or full path. Seperate with | -->
    <add key="ExcludedPaths" value="excludedpath"/>
    <add key="RecursiveSearchEnabled" value="true"/>
    <add key="FileFilter" value="*.cs;*.cshtml;*.js"/>
    <add key="OutputFile" value="..\..\test\output.xlsx"/>
    <add key="OpenOutputFileAfterProcess" value="true"/>
    <add key="SearchPatternFile" value="..\..\test\patterns.txt"/>
  </appSettings>
</configuration>